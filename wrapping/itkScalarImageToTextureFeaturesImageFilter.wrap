  foreach(t ${WRAP_ITK_INT}) 
    itk_wrap_template("${ITKM_I${t}${d}}${ITKM_I${ITKM_V${ITKM_F}8}${d}}" 
                      "${ITKT_I${t}${d}}, ${ITKT_I${ITKT_V${ITKM_F}8}${d}}")
