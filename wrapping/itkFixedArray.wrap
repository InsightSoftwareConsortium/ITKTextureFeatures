itk_wrap_class("itk::FixedArray")
  UNIQUE(vector_dims "8;10;${ITK_WRAP_VECTOR_COMPONENTS}")
  UNIQUE(vector_types "F;${WRAP_ITK_SCALAR}")
  foreach(d ${vector_dims})
    foreach(t ${vector_types})
      itk_wrap_template("${ITKM_FA${t}${d}}" "${ITKT_FA${t}${d}}")
    endforeach()
  endforeach()
itk_end_wrap_class()
